{
    "name": "dyrynda/vagabond",
    "description": "Eloquent without a home",
    "type": "project",
    "homepage": "https://github.com/michaeldyrynda/vagabond",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Dyrynda",
            "email": "michael@dyrynda.com.au"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "dyrynda/nomad": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "App\\VagabondServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "bin-dir": "./"
    }
}
